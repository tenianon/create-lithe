{
	"name": "create-lithe",
	"version": "0.0.2",
	"author": "Nian",
	"description": "scaffolding tool for Lithe Admin",
	"homepage": "https://github.com/tenianon/create-lithe",
	"repository": {
		"type": "github",
		"url": "git+https://github.com/tenianon/create-lithe.git"
	},
	"bugs": {
		"url": "https://github.com/tenianon/create-lithe/issues"
	},
	"license": "MIT",
	"type": "module",
	"bin": {
		"create-lithe": "index.js"
	},
	"scripts": {
		"dev": "bun build src/index.ts --outfile dist/index.mjs --watch",
		"build": "bun build src/index.ts --outfile dist/index.mjs --target node --minify",
		"prepublishOnly": "bun run build",
		"biome:format": "biome format --write src",
		"biome:check": "biome check --write src"
	},
	"keywords": [
		"cli",
		"create",
		"create-lithe",
		"template",
		"lithe-admin"
	],
	"files": [
		"dist"
	],
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@clack/prompts": "^0.11.0",
		"@types/node": "^24.3.0"
	},
	"dependencies": {
		"chalk": "^5.6.0",
		"fflate": "^0.8.2",
		"globby": "^14.1.0"
	}
}
